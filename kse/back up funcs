def Job1():
    if lastruns['timesale'] is None:

        lastruns['timesale'] = datetime.datetime.now()

    diff = (datetime.datetime.now() - lastruns['timesale']).seconds
    if diff >= 10:
        lastruns['timesale'] = datetime.datetime.now()
        # UpdateRunning('TimeSale')
        # TimeSale()


